<Window x:Class="ResourceAZ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResourceAZ"
        xmlns:vm="clr-namespace:ResourceAZ.ViewModels"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="902.5" Width="1439" WindowStartupLocation="CenterScreen">
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Model="{Binding Model}" Margin="916,45,0,0" BorderBrush="Black" VerticalAlignment="Top" Height="246" HorizontalAlignment="Left" Width="472">
            <oxy:PlotView.Effect>
                <DropShadowEffect Color="Gainsboro" ShadowDepth="1" BlurRadius="30"/>
            </oxy:PlotView.Effect>
        </oxy:PlotView>
        <DataGrid HorizontalAlignment="Left" Height="360" Margin="28,45,0,0" VerticalAlignment="Top" 
                ItemsSource="{Binding listMeasure}"  Width="381"/>
    </Grid>
</Window>
